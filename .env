# ========================
# Ticketera - DEV ENV
# ========================

# --- Postgres ---
POSTGRES_IMAGE=postgres:16-alpine
POSTGRES_DB=ticketera_dev
POSTGRES_USER=ticketera_app
POSTGRES_PASSWORD=dev_pass_123
PGDATA_VOL=ticketera_dev_pgdata
PG_PORT=5432

AUTH_DB_SCHEMA=auth_dev
EVENT_DB_SCHEMA=event_dev
TICKET_DB_SCHEMA=ticket_dev
PAYMENT_DB_SCHEMA=payment_dev
NOTIFICATION_DB_SCHEMA=notification_dev
ORDER_DB_SCHEMA=order_dev
GATEWAY_DB_SCHEMA=gateway_dev

# --- Node/Frontend ---
NODE_ENV=development
FRONTEND_PORT=3000

# --- Backend / Spring Boot ---
SPRING_PROFILES_ACTIVE=dev
BACKEND_PORT=8080

# --- Otros servicios que quieras usar en dev ---
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# --- Timezone ---
TZ=America/Argentina/Cordoba
